    "babel-jest": "26.0.1",
    "babel-loader": "8.1.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "jest": "26.0.1",
    "react-test-renderer": "16.13.1",
    "redux-devtools-extension": "2.13.8",
    "redux-mock-store": "1.5.4",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
